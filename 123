pipeline
{
    agent any
    stages
    {
        stage('Cont_Download')
        {
            steps
            {
                git 'https://github.com/malikdaman/Pipieline_Final.git'
            }
        }
        stage('Cont_Build')
        {
            steps
            {
                sh 'mvn package'
            }
        }
        stage('Cont_Deploy')
        {
            steps
            {
                sh 'scp /home/ubuntu/.jenkins/workspace/Devvvvv/webapp/target/webapp.war ubuntu@172.31.29.87:/var/lib/tomcat7/webapps/qaenv.war'
            }
        }
        
    }
    
}
